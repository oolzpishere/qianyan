<p id="notice"><%= notice %></p>

<h1>Dvd Results</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dvd_results.each do |dvd_result| %>
      <tr>
        <td><%= link_to 'Show', dvd_result %></td>
        <td><%= link_to 'Edit', edit_dvd_result_path(dvd_result) %></td>
        <td><%= link_to 'Destroy', dvd_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dvd Result', new_dvd_result_path %>
