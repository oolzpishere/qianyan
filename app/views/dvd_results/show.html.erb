<div class="container">
  <div class="row p-1">
    <div class="col-12">
    </div>
  	<div class="card col-12 px-0">
      <div class="card-header">
        <span class="h6"><%= @dvd_result.form_name %></span>
      </div>
      <div class="card-body">
  	    <div class="card-text">
  			<%  Entry.new(result: @dvd_result).show_values.each do |k, v| %>
  				<p class="mb-1">
            <b><%= "#{k}:"%></b>
  					<%= compile_value(v) %>
  				</p>
  			<% end %>
        <%= link_to '返回', :back, class: "waves-effect waves-light btn blue darken-1 white-text" %>
  	     <%# link_to '返回', { controller: "dvd_results",action: 'index', subject: @dvd_result.class.name}, class: "waves-effect waves-light btn blue darken-1 white-text" %>
  	    </div>
      </div>
  	</div>


  </div>
</div>
