<p id="notice"><%= notice %></p>

<h1>Dvd Requests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dvd_requests.each do |dvd_request| %>
      <tr>
        <td><%= link_to 'Show', dvd_request %></td>
        <td><%= link_to 'Edit', edit_dvd_request_path(dvd_request) %></td>
        <td><%= link_to 'Destroy', dvd_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dvd Request', new_dvd_request_path %>
